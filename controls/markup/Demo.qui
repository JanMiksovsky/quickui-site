<!--
Control demo

This turns off any styling applied to the quickui.org site.
-->

<Control className="Demo">

<content>
    <div ref="demo"/>
</content>

<style>
/* Space out demo elements */
.demo > * {
    margin: 0 0.25em 0.25em 0;
}

.Demo, .Demo input {
    font-family: Helvetica, Arial, sans-serif;
}

h1, h2, h3 {
    font-weight: bold;
}
</style>

<script>
Demo.prototype.extend({
    demo: Control.chain( "$demo" )
});
</script>

</Control>
