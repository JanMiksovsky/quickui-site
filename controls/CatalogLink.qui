<Control name="CatalogLink">

<prototype>
    <Link/>
</prototype>

<script>
CatalogLink.prototype.extend({
    content: function( content ) {
        if ( content !== undefined ) {
            this.href( href="/catalog/default.html#page=" + content + "About" );
        }
        return this._super( content );
    }
});
</script>

</Control>
