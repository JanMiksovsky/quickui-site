<Control name="Tag" tag="span">

<content>&lt;<span id="Tag_content" />&gt;</content>

<style>
{
    display: inline;
    font-family: Consolas, Lucida Console, Courier, Courier New, monospace;
}
</style>

<script>
Tag.prototype.extend({
    content: Control.chain("$Tag_content", "content")
});
</script>

</Control>
